<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="1350.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="servicioSocial.controllers.ConsultarProyectoController">
  <children>
    <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="50.0" layoutX="0.0" layoutY="20.0" stroke="BLACK" strokeType="INSIDE" width="1350.0" />
    <Button fx:id="regresarBT" layoutX="10.0" layoutY="33.0" mnemonicParsing="false" onAction="#regresar" text="Regresar">
      <font>
        <Font name="Arial Bold" size="15.0" fx:id="x1" />
      </font>
    </Button>
    <Label layoutX="537.0" layoutY="30.0" text="Consultar proyecto">
      <font>
        <Font name="Arial Bold" size="30.0" />
      </font>
    </Label>
    <Label fx:id="nombreDependenciaLB" layoutX="500.0" layoutY="75.0" text="">
      <font>
        <Font name="Arial Bold" size="20.0" />
      </font>
    </Label>
    <Label font="$x1" layoutX="100.0" layoutY="125.0" text="Nombre del proyecto:" />
    <TextField fx:id="nombreProyectoTF" editable="false" layoutX="300.0" layoutY="125.0" prefWidth="300.0" />
    <Label font="$x1" layoutX="100.0" layoutY="175.0" text="Descripcion general:" />
    <TextArea fx:id="descripcionTA" editable="false" layoutX="300.0" layoutY="175.0" prefHeight="150.0" prefWidth="300.0" wrapText="true" />
    <Label font="$x1" layoutX="100.0" layoutY="350.0" text="Actividades:" />
    <TextArea fx:id="actividadesTA" editable="false" layoutX="300.0" layoutY="350.0" prefHeight="100.0" prefWidth="300.0" wrapText="true" />
    <TableView fx:id="horarioTV" layoutX="100.0" layoutY="475.0" prefHeight="50.0" prefWidth="875.0">
      <columns>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="125.0" text="Lunes" fx:id="lunesTC" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="125.0" text="Martes" fx:id="martesTC" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="125.0" text="Miércoles" fx:id="miercolesTC" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="125.0" text="Jueves" fx:id="juevesTC" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="125.0" text="Viernes" fx:id="viernesTC" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="125.0" text="Sábado" fx:id="sabadoTC" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="125.0" text="Domingo" fx:id="domingoTC" />
      </columns>
    </TableView>
    <Label font="$x1" layoutX="775.0" layoutY="125.0" text="Nombre del Jefe de Proyecto:" />
    <TextField fx:id="nombreJefeProyectoTF" editable="false" layoutX="1000.0" layoutY="125.0" prefWidth="300.0" />
    <Label font="$x1" layoutX="775.0" layoutY="175.0" text="Cargo:" />
    <TextField fx:id="cargoTF" editable="false" layoutX="1000.0" layoutY="175.0" prefWidth="300.0" />
    <Label font="$x1" layoutX="775.0" layoutY="225.0" text="Correo Electronico:" />
    <TextField fx:id="correoElectronicoTF" editable="false" layoutX="1000.0" layoutY="225.0" prefWidth="300.0" />
  </children>
</AnchorPane>
