<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="1350.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="servicioSocial.controllers.DocumentosYReportesController">
  <children>
    <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="50.0" layoutX="0.0" layoutY="20.0" stroke="BLACK" strokeType="INSIDE" width="1350.0" />
    <MenuBar fx:id="menuMB" layoutX="10.0" layoutY="33.0">
      <menus>
        <Menu mnemonicParsing="false" text="Menú">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#irAProyectos" text="Proyectos" />
            <MenuItem mnemonicParsing="false" text="Documentos" />
            <MenuItem mnemonicParsing="false" onAction="#salir" text="Salir" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <Label layoutX="502.0" layoutY="30.0" text="Documentos y Reportes">
      <font>
        <Font name="Arial Bold" size="30.0" />
      </font>
    </Label>
    <TextField fx:id="matriculaTF" layoutX="450.0" layoutY="100.0" prefHeight="25.0" prefWidth="300.0" promptText="Matrícula" text="" />
    <Button fx:id="buscarBT" layoutX="800.0" layoutY="100.0" mnemonicParsing="false" text="Buscar">
      <font>
        <Font name="Arial Bold" size="15.0" fx:id="x1" />
      </font>
    </Button>
    <Label fx:id="alumnoLB" font="$x1" layoutX="400.0" layoutY="175.0" text="" />
    <Label fx:id="matriculaLB" font="$x1" layoutX="800.0" layoutY="175.0" text="" />
    <Label layoutX="100.0" layoutY="250.0" text="Documentos">
      <font>
        <Font name="Arial Bold" size="20.0" fx:id="x3" />
      </font>
    </Label>
    <Label font="$x1" layoutX="150.0" layoutY="300.0" text="Horario:" />
    <Button fx:id="subirHorarioBT" layoutX="350.0" layoutY="300.0" mnemonicParsing="false" onAction="#subirHorario" text="Subir">
      <font>
        <Font name="Arial Bold" size="12.0" fx:id="x2" />
      </font>
    </Button>
    <Button fx:id="modificarHorarioBT" font="$x2" layoutX="450.0" layoutY="300.0" mnemonicParsing="false" onAction="#modificarHorario" text="Modificar" />
    <Label font="$x1" layoutX="150.0" layoutY="354.0" text="Carta de Presentación:" />
    <Button fx:id="subirCartaPresentacionBT" font="$x2" layoutX="350.0" layoutY="350.0" mnemonicParsing="false" onAction="#subirCartaPresentacion" text="Subir" />
    <Button fx:id="modificarCartaPresentacionBT" font="$x2" layoutX="450.0" layoutY="350.0" mnemonicParsing="false" onAction="#modificarCartaPresentacion" text="Modificar" />
    <Label font="$x1" layoutX="150.0" layoutY="400.0" text="Carta de Aceptación:" />
    <Button id="subirCartaAceptacion" fx:id="subirCartaAceptacionBT" font="$x2" layoutX="350.0" layoutY="400.0" mnemonicParsing="false" onAction="#subirCartaAcpetacion" text="Subir" />
    <Button fx:id="modificarCartaAceptacionBT" font="$x2" layoutX="450.0" layoutY="400.0" mnemonicParsing="false" onAction="#modificarCartaAceptacion" text="Modificar" />
    <Label font="$x1" layoutX="150.0" layoutY="450.0" text="Plan de Actividades:" />
    <Button fx:id="subirPlanActividadesBT" font="$x2" layoutX="350.0" layoutY="450.0" mnemonicParsing="false" onAction="#subirPlanActividades" text="Subir" />
    <Button fx:id="modificarPlanActividadesBT" font="$x2" layoutX="450.0" layoutY="450.0" mnemonicParsing="false" onAction="#modificarPlanActividades" text="Modificar" />
    <Label font="$x1" layoutX="150.0" layoutY="500.0" text="Carta de Liberación:" />
    <Button fx:id="subirCartaLiberacionBT" font="$x2" layoutX="350.0" layoutY="500.0" mnemonicParsing="false" onAction="#subirCartaLiberacion" text="Subir" />
    <Button fx:id="modificarCartaLiberacionBT" font="$x2" layoutX="450.0" layoutY="500.0" mnemonicParsing="false" onAction="#modificarCartaLiberacion" text="Modificar" />
    <Label font="$x1" layoutX="150.0" layoutY="550.0" text="Reporte final:" />
    <Button fx:id="subirReporteFinalBT" font="$x2" layoutX="350.0" layoutY="550.0" mnemonicParsing="false" onAction="#subirReporteFinal" text="Subir" />
    <Button fx:id="modificarReporteFinalBT" font="$x2" layoutX="450.0" layoutY="550.0" mnemonicParsing="false" onAction="#modificarReporteFinal" text="Modificar" />
    <Label font="$x3" layoutX="675.0" layoutY="250.0" text="Reportes" />
    <TableView fx:id="reporteTV" layoutX="725.0" layoutY="300.0" prefHeight="250.0" prefWidth="400.0">
      <columns>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="400.0" text="Reporte" fx:id="nombreTC" />
      </columns>
    </TableView>
    <Button fx:id="subirReporteBT" font="$x2" layoutX="850.0" layoutY="600.0" mnemonicParsing="false" onAction="#subirReporte" text="Subir" />
    <Button fx:id="modificarReporteBT" font="$x2" layoutX="950.0" layoutY="600.0" mnemonicParsing="false" onAction="#modificarReporte" text="Modificar" />
  </children>
</AnchorPane>
